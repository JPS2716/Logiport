{
  "version": 2,
  "builds": [
    {
      "src": "index_fixed.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*\\.(png|jpg|jpeg|svg|gif))", "dest": "/public/$1" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "index_fixed.js" }
  ]
}
